<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="382.0" prefWidth="660.0" spacing="20.0" style="-fx-background-color: c2c2c2;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HelloWorld.controllerFX.MostrarArticulosController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <TableView fx:id="tblArticulos" prefHeight="281.0" prefWidth="524.0" style="-fx-border-color: white;">
        <columns>
          <TableColumn fx:id="colCodigo" prefWidth="131.0" text="Codigo" />
          <TableColumn fx:id="colDescripcion" prefWidth="146.0" text="Descripción" />
          <TableColumn fx:id="colPvp" prefWidth="64.0" text="PVP" />
          <TableColumn fx:id="colGastos" prefWidth="149.0" text="Gastos de Envío" />
          <TableColumn fx:id="colTiempo" prefWidth="129.0" text="Tiempo de Envío" />
        </columns>
      </TableView>
      <Button fx:id="btnVolver" mnemonicParsing="false" onAction="#onClickVolver" style="-fx-background-color: #646464;" text="Volver" textFill="#c2c2c2">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</VBox>
