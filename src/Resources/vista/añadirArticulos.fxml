<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="522.0" prefWidth="660.0" spacing="20.0" style="-fx-background-color: c2c2c2;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="HelloWorld.controllerFX.AñadirArticuloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label prefHeight="21.0" prefWidth="152.0" style="-fx-background-color: c2c2c2;" text="Código del artículo" textFill="#646464">
         <font>
            <Font size="15.0" />
         </font></Label>
      <TextField fx:id="txtCodigo" prefHeight="25.0" prefWidth="517.0" />
      <Label layoutX="30.0" layoutY="30.0" prefHeight="21.0" prefWidth="164.0" text="Descripción del artículo" textFill="#646464">
         <font>
            <Font size="15.0" />
         </font></Label>
      <TextField fx:id="txtDescripcion" layoutX="30.0" layoutY="231.0" />
      <Label layoutX="30.0" layoutY="82.0" prefHeight="32.0" prefWidth="130.0" text="PVP" textFill="#646464">
         <font>
            <Font size="15.0" />
         </font></Label>
      <TextField fx:id="txtPvp" layoutX="30.0" layoutY="82.0" />
      <Label layoutX="30.0" layoutY="194.0" prefHeight="32.0" prefWidth="130.0" text="Gastos de envío" textFill="#646464">
         <font>
            <Font size="15.0" />
         </font></Label>
      <TextField fx:id="txtGastosEnvio" layoutX="30.0" layoutY="231.0" />
      <Label layoutX="30.0" layoutY="276.0" prefHeight="32.0" prefWidth="130.0" text="Tiempo de envío" textFill="#646464">
         <font>
            <Font size="15.0" />
         </font></Label>
      <TextField fx:id="txtTiempoEnvio" layoutX="30.0" layoutY="313.0" />
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Button fx:id="btnAceptar" mnemonicParsing="false" onAction="#onClickAceptar" prefHeight="30.0" prefWidth="75.0" style="-fx-background-color: #646464;" text="Añadir" textFill="#c2c2c2">
               <font>
                  <Font size="14.0" />
               </font></Button>
         </children>
      </VBox>
   </children>
</VBox>
